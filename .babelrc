{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-typescript",
      {
        "jsxPragma": "VDom.createElement"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "classic",
        "pragma": "VDom.createElement",
        "pragmaFrag": "VDom.Fragment"
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}
