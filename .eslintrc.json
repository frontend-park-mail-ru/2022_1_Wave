{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["*.precompile.js"],
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "import/no-absolute-path": "off",
        "import/no-unresolved": "off",
        "class-methods-use-this": "off",
        "no-undef": "off"
    }
}
